	<parameter name="ENQID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT hdr.po_no,
  hdr.po_date,
  hdr.po_remark,
  hdr.basic_total,
  decode (hdr.packing_prcnt,0,'---',hdr.packing_prcnt) packing_prcnt,
  decode (hdr.packing_value,0,'---',hdr.packing_value) packing_value,
  hdr.sub_total,
  decode (hdr.vat_prcnt,0,'---',hdr.vat_prcnt) vat_prcnt,
  decode (hdr.vat_value,0,'---',hdr.vat_value) vat_value,
  decode (hdr.cst_prcnt,0,'---',hdr.cst_prcnt) cst_prcnt,
  decode (hdr.cst_value,0,'---',hdr.cst_value) cst_value,
  hdr.sub_tax_total,
  decode (hdr.freight_prcnt,0,'---',hdr.freight_prcnt) freight_prcnt,
  decode (hdr.freight_value,0,'---',hdr.freight_value) freight_value,
  decode (hdr.others_prcnt,0,'---',hdr.others_prcnt) others_prcnt,
  decode (hdr.others_value,0,'---',hdr.others_value) others_value,
  decode (hdr.ed_prcnt,0,'---',hdr.ed_prcnt) ed_prcnt,
  decode (hdr.ed_value,0,'---',hdr.ed_value) ed_value,
  decode (hdr.cess_prcnt,0,'---',hdr.cess_prcnt) cess_prcnt,
  decode (hdr.cess_value,0,'---',hdr.cess_value) cess_value,
  decode (hdr.hed_prcnt,0,'---',hdr.hed_prcnt) hed_prcnt,
  decode (hdr.hed_value,0,'---',hdr.hed_value) hed_value,
  hdr.grand_total,
  v.vendor_name,
  v.vendor_address,
  v.vendor_code,
  v.contact_name ||' , '|| v.desigination contact_name,
  dtl.required_qty,
  hdr.payment_terms,
  hdr.freight_terms,
  hdr.delivery_terms,
  m.materail_name,
  dtl.po_qty ||' '|| dtl.material_uom rate_uom,
  dtl.unit_rate,
  dtl.basic_value,
  ltrim(pkg_app_utils.fn_amount_to_words(ROUND(hdr.grand_total,2))) amt_words
FROM t_mms_po_hdr hdr,
  t_mms_po_dtl dtl,
  m_base_vendor v,
  m_mms_material m
WHERE hdr.po_id  = dtl.po_id
AND v.vendor_id  = hdr.vendor_id
AND m.material_id=dtl.material_id
AND hdr.po_id    =$P{ENQID}]]>
	</queryString>
