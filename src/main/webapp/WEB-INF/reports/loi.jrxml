	<queryString>
		<![CDATA[select
loihdr.loi_number,
loihdr.ref_date ,
v.vendor_name,
v.vendor_address||','||c.city_name ||','||s.state_name,
v.contact_number,loihdr.subject,
loihdr.remarks,
m.materail_name ,
loidtl.qty,
loidtl.unit_rate,
loidtl.remarks dtlremarks,
loidtl.basic_value,
decode (loidtl.discount_val,0,'---',loidtl.discount_val) discount_val,
decode (loidtl.discount_per,0,'---',loidtl.discount_per) discount_per,
v.contact_name ||' , '|| v.desigination contact_name,
loihdr.basic_total,
decode (loihdr.packing_prcnt,0,'---',loihdr.packing_prcnt) packing_prcnt,
decode (loihdr.packing_value,0,'---',loihdr.packing_value) packing_value,

  loihdr.sub_total,
decode (loihdr.vat_prcnt,0,'---',loihdr.vat_prcnt) vat_prcnt,
decode (loihdr.vat_value,0,'---',loihdr.vat_value) vat_value,
decode (loihdr.cst_prcnt,0,'---',loihdr.cst_prcnt) cst_prcnt,
decode (loihdr.cst_value,0,'---',loihdr.cst_value) cst_value,
  loihdr.sub_tax_total,
decode (loihdr.freight_prcnt,0,'---',loihdr.freight_prcnt) freight_prcnt,
decode (loihdr.freight_value,0,'---',loihdr.freight_value) freight_value,
decode (loihdr.others_prcnt,0,'---',loihdr.others_prcnt) others_prcnt,
decode (loihdr.others_value,0,'---',loihdr.others_value) others_value,

decode (loihdr.ed_prcnt,0,'---',loihdr.ed_prcnt) ed_prcnt,
decode (loihdr.ed_value,0,'---',loihdr.ed_value) ed_value,
decode (loihdr.cess_prcnt,0,'---',loihdr.cess_prcnt) cess_prcnt,
decode (loihdr.cess_value,0,'---',loihdr.cess_value) cess_value,
decode (loihdr.hed_prcnt,0,'---',loihdr.hed_prcnt) hed_prcnt,
decode (loihdr.hed_value,0,'---',loihdr.hed_value) hed_value,
  loihdr.grand_total,
  ltrim(pkg_app_utils.fn_amount_to_words(ROUND(loihdr.grand_total,2))) amt_words

from t_mms_loi_hdr loihdr,
t_mms_loi_dtl loidtl,m_base_vendor v,m_mms_material m,m_base_city c,m_base_state s
where loihdr.loi_hdr_id = loidtl.loi_hdr_id
and v.vendor_id=loihdr.vendor_id
and m.material_id=loidtl.mat_id
and c.city_id=v.city_id
and s.state_id=v.state_id
and loihdr.loi_hdr_id=$P{LOIID}]]>
	</queryString>
