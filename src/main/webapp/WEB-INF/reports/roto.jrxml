	<queryString>
		<![CDATA[SELECT h.pulviz_ref_no ,
  h.pulviz_date pulviz_ref_date,
  d.prodn_date pulviz_date,
  d.balance_qty pulviz_blance_mat,
  NVL(d.oee_prcnt,0) pulviz_oee,
  d.balance_prcnt pulviz_quality,
  d.remarks,
  m.asset_name,
  d.mchn_starttime machine_on_time,
  d.mchn_stoptime machine_off_time,
  d.input_qty machine_input,
  d.output_qty machine_output
FROM t_stt_mfg_pulviz_hdr h
,t_stt_mfg_pulviz_dtl d,t_ams_asset_details m
where h.pulviz_id = d.pulviz_id
and m.asset_id = d.machine_id
AND h.pulviz_id   =$P{rotoid}]]>
	</queryString>
