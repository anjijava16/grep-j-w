	<queryString>
		<![CDATA[SELECT NVL(SUM(closebal),'0') closebal,
  NVL(SUM(hourtot),'0') hourstotal,
  NVL(SUM(mintot),'0') mintotal,
  NVL(SUM(purchaselitres),'0') purchaselitre,
  NVL(SUM(consumption),'0') consumption,
  NVL(SUM(totalamount),'0') totalamount,
  gendate,monthnum
FROM
  (SELECT regexp_substr(TOTAL_TIME,'^[^:]*') hourtot,
    TO_CHAR(GENSET_DATE,'month') gendate,to_char(genset_date,'MM') monthnum,
    REGEXP_SUBSTR(TOTAL_TIME, '[^:]+$') mintot,
    DIESEL_CLOSE_BALANCE closebal,
    DIESEL_PURCHASE_LITERS purchaselitres,
    DIESEL_CONSUPTION_BALANCE consumption,
    GENSET_DATE,
    TOTAL_COST totalamount,
    TOTAL_TIME
  FROM t_generator
  )
WHERE TO_CHAR(GENSET_DATE,'RRRR')=$P{year}
GROUP BY gendate, monthnum
ORDER BY monthnum]]>
	</queryString>
