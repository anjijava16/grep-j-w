	<queryString>
		<![CDATA[select wf.enquiry_wf_id enquiry_wf_id, wf.rework_on wf_date,enqh.enquiry_no,
enqh.enquiry_date,client.client_name,enqd.cust_fld1 part_number,enqs.spec_code,enqs.spec_desc,
allto.first_name alottedto,wf.target_date,wf.completed_date ,aprvby.first_name auth_person,
wf.bottom_top_case_model,wf.exist_case_model,wf.fusioned_case_model,wf.new_die_case_model
from t_stt_enquiry_workflow wf,t_sms_enquiry_hdr enqh,m_crm_clients client,t_sms_enquiry_dtl enqd ,
t_sms_enquiry_spec enqs,m_base_employee allto,m_base_employee aprvby
where
enqh.enquiry_id=wf.enquiry_id
and enqh.enquiry_id = enqd.enquiry_id
and enqh.client_id = client.client_id
and enqs.enquiry_dtlid=enqd.enquiry_dtlid
and allto.employee_id=wf.pending_with
and aprvby.employee_id(+)=wf.design_auth_by
and wf.enquiry_wf_id =$P{WFID}]]>
	</queryString>
