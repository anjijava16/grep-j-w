	<queryString>
		<![CDATA[SELECT enqhdr.ENQUIRY_DATE,
  enqhdr.ENQUIRY_NO,
  enqdtl.CUST_PROD_DESC,
  cust.CLIENT_NAME customer_name,
  enqs.spec_code ||' : '|| enqs.spec_desc specif,

  p.PRODUCT_NAME,
  city.CITY_NAME,
  enqdtl.ENQRY_QTY
FROM T_SMS_ENQUIRY_HDR enqhdr,
  T_SMS_ENQUIRY_DTL enqdtl,
  M_CRM_CLIENTS cust,
  t_sms_enquiry_spec enqs,
  M_BASE_PRODUCT p,
  M_BASE_CITY city

WHERE enqhdr.ENQUIRY_ID= enqdtl.ENQUIRY_ID
and enqdtl.ENQUIRY_ID= enqs.ENQUIRY_ID
AND enqhdr.CLIENT_ID   =cust.CLIENT_ID
AND city.CITY_ID       =cust.CITY_ID
AND p.PRODUCT_ID       =enqdtl.PRODUCT_ID
AND enqdtl.PRODUCT_ID  =enqs.PRODUCT_ID
AND enqs.ENQUIRY_ID  =$P{ENQID}]]>
	</queryString>
