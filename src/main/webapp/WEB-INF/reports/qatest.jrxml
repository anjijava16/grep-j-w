		<![CDATA[select qah.inspection_no,qah.inspection_dt,cl.client_code , cl.client_name client_name,
prod.product_code ||' & '|| prod.product_name product_name,qah.prod_slno,qah.test_observation,qah.test_result,ts.test_spec,
qad.test_spcreslt,qad.qa_remarks
from T_MFG_QA_TEST_HDR qah,t_mfg_qa_test_dtl qad,m_crm_clients cl,m_base_product prod,m_mfg_qa_test_spec ts
