AND pe.employee_id = $P{staffid}
AND pe.employee_id = se.employee_id
AND pe.emp_earn_id = se.emp_earn_id
AND se.earn_id = e.earn_id
AND e.earn_code = 'GROSS'
AND e.earn_status = 'Active'
AND emp_earn_status = 'Active')
order by sort_order]]>
