	<queryString>
		<![CDATA[select ecr.ecr_number ecr_number,ecr.ecr_date ecr_date,client.client_name,ecr.part_number part_number,ecr.drg_number drg_number,
dept1.dept_name from_dept,dept2.dept_name to_dept,ecr.change_detail change_detail,ecr.change_reason change_reason,
ecr.if_any_test if_any_test
from t_stt_ec_request ecr,m_crm_clients client,m_base_department dept1,m_base_department dept2,m_base_product prod
where
ecr.client_id = client.client_id
and ecr.from_dept_id = dept1.dept_id
and ecr.to_dept_id = dept2.dept_id
and ecr.product_id = prod.product_id
and ecr.ecr_id=$P{ECRID}]]>
	</queryString>
