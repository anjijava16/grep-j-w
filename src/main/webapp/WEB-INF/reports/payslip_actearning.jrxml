	<queryString>
		<![CDATA[select payroll_earn_id,earn_desc, earn_amount
FROM t_hcm_payroll_earn
WHERE payroll_id = $P{processid}
 AND employee_id = $P{staffid}
AND emp_earn_id NOT IN (
SELECT se.emp_earn_id
FROM t_hcm_payroll_earn pe, t_hcm_employee_earning se, m_hcm_earnings e
WHERE pe.payroll_id = $P{processid}
AND pe.employee_id = $P{staffid}
AND pe.employee_id = se.employee_id
AND pe.emp_earn_id = se.emp_earn_id
AND se.earn_id = e.earn_id
AND e.earn_code = 'GROSS'
AND e.earn_status = 'Active'
AND emp_earn_status = 'Active')
order by sort_order]]>
