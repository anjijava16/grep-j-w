		<![CDATA[select hdr.po_no,hdr.po_date,v.vendor_name,v.vendor_address,v.vendor_code,hdr.payment_terms,hdr.freight_terms,
hdr.delivery_terms,m.materail_name,dtl.po_qty,dtl.unit_rate,dtl.basic_value,
