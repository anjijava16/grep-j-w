	<queryString>
		<![CDATA[SELECT ass.asset_name,g.GENSET_DATE,g.DIESEL_OPEN_BALANCE,
  g.TOTAL_TIME, g.DIESEL_CLOSE_BALANCE,
  g.TOTAL_COST totalcost,g.ONE_LTRCOST,g.DIESEL_CONSUPTION_BALANCE,
  g.DIESEL_PURCHASE_LITERS purchaselitre
FROM t_ams_generator g,t_ams_asset_details ass
where
g.asset_id = ass.asset_id
and GENSET_DATE between to_date($P{startdate},'DD-Mon-RRRR') and to_date($P{enddate},'DD-Mon-RRRR')
and g.asset_id like COALESCE($P{assetid},'%')
order by genset_date desc]]>
	</queryString>
