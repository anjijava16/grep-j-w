	<queryString>
		<![CDATA[SELECT a.INSPECTION_DATE,
  a.BOX_WEIGHT,
  a.TOTAL_QTY,
  a.NO_OF_SAMPLE,
  a.OBSERVATION,
  lift_status(a.status),
  b.PARAM0,
  b.PARAM1,
  b.PARAM2,
 b.PARAM3,
  b.REMARKS,
  c.customer_code,
  d.wo_number,
  e.box_ref_name
FROM T_QA_RAIN_TEST_HDR a
JOIN T_QA_RAIN_TEST_DTL b
ON a.RAIN_TEST_HDR_ID = b.RAIN_TEST_HDR_ID
JOIN M_CUSTOMER c
ON a.customer_id = c.customer_id
JOIN T_WO_HDR d
ON c.customer_id = d.customer_id
JOIN T_PRODUCTION_ROTO_PROD_DTL e
on a.ROTO_PROD_ID = e.ROTO_PROD_ID
join T_WO_INDENT_ISSUE_DTL f
on e.wo_issue_prod_id = f.wo_issue_prod_id
WHERE a.RAIN_TEST_HDR_ID =$P{Rainheaderid}]]>
