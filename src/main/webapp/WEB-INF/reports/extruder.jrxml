	<parameter name="headerid" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT h.extrud_id,d.extrud_dtlid,h.grade_no,
  h.lot_no,
  h.EXTRUD_REF_NO,
  h.extrud_date,d.charge_endtime CHRG_OFF_TIME,
  d.output_qty,d.charge_sttime chrg_on_time,
  nvl(round(d.oee_prnct,2),'0') oeeevalue,
  d.remarks,mat.acc_lot_no,nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,1),'0') lldpe,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,2),'0') md,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,3),'0') additives,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,4),'0') regrined FROM
t_stt_mfg_extrud_hdr h
JOIN t_stt_mfg_extrud_dtl d
ON(h.extrud_id =d.extrud_id)
join (select extrud_dtlid,listagg(m.MATERIAL_QTY, ',')
within group (order by m.extrud_dtlid) acc_lot_no
from t_stt_mfg_extrud_mtrl m
where extrud_id=$P{headerid} group by extrud_dtlid) mat on(mat.extrud_dtlid=d.extrud_dtlid)
AND h.extrud_id=$P{headerid}]]>
	</queryString>
