	<queryString>
		<![CDATA[select poh.PO_ID PO_NUMBER,poh.PO_DATE PO_DATE,poh.CLIENT_ID CUSTOMER_ID,poh.PACKING_PRCNT PACKING_PERCENTAGE,poh.PACKING_VALUE PACKING_VALUE,poh.ED_PRCNT ED,poh.ED_VALUE ED_VALUE,poh.OTHERS_PRCNT other_charge,poh.PDC_CHARGES pdc_values,
       poh.HED_PRCNT HED,poh.HED_VALUE HED_VALUE,poh.CESS_PRCNT CESS,poh.CESS_VALUE,poh.CST_PRCNT CST,poh.CST_VALUE,poh.SHIPPING_ADDR SHIP_TO_ADDRESS,poh.VAT_PRCNT VAT,poh.VAT_VALUE,poh.BASIC_TOTAL BASIC_VALUE,
       poh.SUB_TOTAL,poh.SUB_TAX_TOTAL,poh.GRAND_TOTAL,poh.DUTY_EXEMPTED,poh.CFORM_REQD,poh.FREIGHT_VALUE FREIGHT_CHARGE, pod.PO_QTY qty,
       pod.UNIT_RATE unit_price,pod.BASIC_VALUE value, cust.CLIENT_NAME customer_name,nvl(cust.CLNT_ADDRESS,'---') address1,
       nvl(cust.phone_no,'---') telephone ,nvl(cust.post_code,'---') zipcode, comp.CST_NO cst,comp.ECC_NO ecc,comp.TIN_NO tin,city.CITY_NAME as city_name, state.STATE_NAME as state_name,
       ctry.COUNTRY_NAME as country_name,
       poh.FREIGHT_TERMS as freight_desc,  poh.PAYMENT_TERMS payment_term_desc, poh.WARRANTY_TERMS warranty_desc, poh.DELIVERY_TERMS delivery_desc, pod.CUST_PROD_DESC prod_name, rownum sno,
       ltrim(pkg_app_utils.fn_amount_to_words(round(poh.GRAND_TOTAL,2))) amt_words,
       nvl(pod.CUST_FLD1,'---') cus_part_no,nvl(pod.CUST_FLD1,'---') saarc_part_no
 from T_SMS_PO_HDR poh, T_SMS_PO_DTL pod, M_CRM_CLIENTS cust, M_BASE_CITY city,M_BASE_STATE state,M_BASE_COMPANY comp,
      M_BASE_COUNTRY ctry,T_SMS_ENQUIRY_HDR enq,M_BASE_PRODUCT prd
where poh.PO_ID = pod.PO_ID
and enq.ENQUIRY_ID=poh.ENQUIRY_ID
  and poh.CLIENT_ID = cust.CLIENT_ID
  and comp.COMPANY_ID=poh.COMPANY_ID
 and  cust.CITY_ID = city.CITY_ID(+)
  and  cust.STATE_ID = state.STATE_ID
  and  cust.COUNTRY_ID = ctry.COUNTRY_ID

  and pod.PRODUCT_ID = prd.PRODUCT_ID
  and poh.PO_ID = $P{poid}]]>
	</queryString>
