	<parameter name="DCID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select dch.dc_no dc_no,dch.dc_date dc_date,
decode(dcd.goods_type,'Material',mat.materail_name,'Product',prod.product_name) as description,
decode(dch.dc_type,'Customer',client.client_name,'Vendor',vendor.vendor_name) as contname,
decode(dch.dc_type,'Customer',client.clnt_address,'Vendor',vendor.vendor_address) as address,
dcd.dc_qty dc_qty,dch.mode_of_trans mode_of_trans,dch.remarks,dch.person_name
from t_mms_dc_hdr dch,t_mms_dc_dtl dcd,m_mms_material mat,m_base_product prod,m_crm_clients client,m_base_vendor vendor
where dch.dc_id = dcd.dc_id
and dcd.material_id=mat.material_id(+)
and dcd.product_id=prod.product_id(+)
and dch.customer_id=client.client_id(+)
and dch.vendor_id=vendor.vendor_id(+)
and dcd.dc_id =$P{DCID}]]>
	</queryString>
