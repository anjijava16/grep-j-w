	<parameter name="staffid" class="java.lang.String"/>
	<parameter name="processid" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT ph.payroll_id,
  c.company_name,
  ph.processed_dt,
  pd.employee_id,
  s.first_name||' '||s.last_name first_name,
  pd.working_days,
  pd.no_of_leaves,
  pd.lwp_days,
  pd.total_earn,
  pd.total_dedn,
  pd.net_pay,
  s.employee_code,
  d.dept_name,
  ds.designation_name,
  b.branch_name,
  TO_CHAR(s.doj,'DD-Mon-RRRR') doj,
  bb.IFSC_CODE as bank_branchname,
  pd.bank_acc_no,
  g.grade_desc,
  DECODE(s.gender,'M','Male','Female') gender,
  (SELECT 'Pay Period '
    ||TO_CHAR(att_start_dt,'DD-Mon-RRRR')
    ||' to '
    ||TO_CHAR(att_end_dt,'DD-Mon-RRRR')
  FROM t_hcm_attendence_proc ap,
    t_hcm_payroll_att_ids pai
  WHERE ap.att_proc_id = pai.att_proc_id
  AND pai.payroll_id   = $P{processid}
  AND rownum           =1
  ) pay_duration,
  (SELECT se.earn_amount
  FROM t_hcm_employee_earning se,
    m_hcm_earnings e
  WHERE se.employee_id   = $P{staffid}
  AND se.earn_id      = e.earn_id
  AND e.earn_code     = 'GROSS'
  AND e.earn_status   = 'Active'
  AND emp_earn_status = 'Active'
  ) std_earn_amount
FROM t_hcm_payroll_hdr ph,
  t_hcm_payroll_dtl pd,
  m_base_employee s,
  m_base_company c,
  m_base_department d,
  m_hcm_designation ds,
  m_base_branch b,
  m_fms_bank_branch bb,
  m_hcm_grade g
WHERE ph.payroll_id   = pd.payroll_id
AND pd.employee_id       = s.employee_id
AND ph.company_id     = c.company_id
AND s.dept_id         = d.dept_id
AND pd.designation_id = ds.designation_id
AND pd.branch_id      = b.branch_id
AND pd.bank_branch_id = bb.bank_branch_id(+)
AND ds.grade_id       = g.grade_id
AND pd.payroll_id     = $P{processid}
AND pd.employee_id       = $P{staffid}]]>
	</queryString>
