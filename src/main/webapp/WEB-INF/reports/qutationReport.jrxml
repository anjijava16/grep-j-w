	<queryString>
		<![CDATA[select qth.QUOTE_NUMBER, qth.QUOTE_DATE,cust.CLIENT_NAME CUSTOMER_NAME, qth.BASIC_TOTAL, qth.PACKING_PRCNT PACKING,qth.PACKING_VALUE,qth.SUB_TOTAL,qth.OTHERS_VALUE OTHER_CHARGES,qth.PDC_CHARGES PDC,
      qth.DUTY_EXEMPTED, qth.ED_PRCNT ED, qth.ED_VALUE, qth.HED_PRCNT HED, qth.HED_VALUE HED_VALUE, qth.CESS_PRCNT CESS, qth.CESS_VALUE, qth.SUB_TAX_TOTAL,
      qth.CST_PRCNT CST, qth.CST_VALUE, qth.VAT_PRCNT VAT, qth.VAT_VALUE, qth.FREIGHT_VALUE FREIGHT_CHARGE, qth.GRAND_TOTAL, qtd.PRODUCT_ID,
      nvl(qtd.CUST_FLD1,'---') cus_part_no, qtd.UNIT_RATE UNIT_PRICE, qtd.QUOTE_QTY as QTY,qtd.BASIC_VALUE,qtd.PDC_VALUE, cust.CLIENT_NAME,cust.CLNT_ADDRESS,
      nvl(cust.PHONE_NO,'---') telephone,nvl(cust.POST_CODE,'---') zipcode, city.CITY_NAME, state.STATE_NAME, ctry.COUNTRY_NAME,
	qtd.CUST_PROD_DESC prod_name, qth.FREIGHT_TERMS FREIGHT_DESC, qth.PAYMENT_TERMS PAYMENT_TERM_DESC, qth.WARRANTY_TERMS WARRANTY_DESC, qth.DELIVERY_TERMS DELIVERY_DESC,enqh.ENQUIRY_NO,enqh.ENQUIRY_DATE,
      ltrim(pkg_app_utils.fn_amount_to_words(round(qth.grand_total,2))) amt_words,qth.QUOTE_TYPE QUOTATIONTYPE,pro.PRODUCT_CODE ||' & ' || qtd.CUST_FLD2  saarcpotno,cust.FAX_NO FAX_NO,qth.QUOTE_VALID_DT QUOTE_VALID_DT,qth.QUOTE_REF QUOTE_REF,nvl(qth.TEC_PERSON,'------') TEC_PERSON,nvl(qth.COMER_PERSON,'------') COMER_PERSON

from T_SMS_QUOTE_HDR qth,T_SMS_QUOTE_DTL qtd,M_CRM_CLIENTS cust,M_BASE_CITY city,M_BASE_STATE state, M_BASE_COUNTRY ctry,M_BASE_PRODUCT pro,T_SMS_ENQUIRY_HDR enqh

where qth.QUOTE_ID = qtd.QUOTE_ID
  and  qtd.PRODUCT_ID =  pro.PRODUCT_ID
  and  qtd.QUODTL_STATUS = 'Active'
  and  qth.ENQUIRY_ID = enqh.ENQUIRY_ID
  and  cust.CITY_ID = city.CITY_ID
  and  cust.STATE_ID = state.STATE_ID
  and  cust.COUNTRY_ID = ctry.COUNTRY_ID
  and cust.CLIENT_ID=enqh.CLIENT_ID
  and  qth.QUOTE_ID =$P{QTID}]]>
	</queryString>
