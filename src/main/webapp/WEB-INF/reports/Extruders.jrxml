	<parameter name="headerid" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT h.extrud_id,d.extrud_dtl_id,h.grade_no,
  h.lot_no,h.batch_no,
  h.regrined_flag,h.EXTRUD_REF_NO,h.EXTRUD_SECTION,
  d.extrud_date,d.CHRG_OFF_TIME,
  d.output_qty,d.chrg_on_time,d.chrg_on_time,
  d.temp_z1,d.temp_z5,nvl(round(d.oee_value,2),'0') oeeevalue,
  d.remarks,mat.acc_lot_no,nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,1),'0') lldpe,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,2),'0') md,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,3),'0') additives,
  nvl(regexp_substr(mat.acc_lot_no,'[^,]+',1,4),'0') regrined FROM
t_production_extrud_hdr h
JOIN t_production_extrud_dtl d
ON(h.EXTRUD_ID =d.extrud_id)
join (select extrud_dtl_id,listagg(m.INPUT_QTY, ',')
within group (order by m.extrud_dtl_id) acc_lot_no
from t_production_extrud_mat_dtl m
where extrud_id=$P{headerid} group by extrud_dtl_id) mat on(mat.extrud_dtl_id=d.extrud_dtl_id)
AND h.extrud_id=$P{headerid}]]>
