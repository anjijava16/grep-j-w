	<queryString>
		<![CDATA[select invhdr.INVOICE_ID as INV_NBR, invhdr.INVOICE_DT as INV_DATE,invhdr.CLIENT_ID as CUSTOMER_ID,invhdr.PACKING_PRCNT as PACKING_PERCENTAGE,invhdr.PACKING_VALUE as PACKING_VALUE,invhdr.ED_PRCNT as ED,
       invhdr.ED_VALUE as ED_VALUE, invhdr.HED_PRCNT as HED,invhdr.HED_VALUE as HED_VALUE,invhdr.CESS_PRCNT as CESS,invhdr.CESS_VALUE as CESS_VALUE,invhdr.CST_PRCNT as CST,invhdr.CST_VALUE as CST_VALUE,
       invhdr.SHIPPING_ADDR as SHIP_TO_ADDRESS, invhdr.VAT_PRCNT as VAT, invhdr.VAT_VALUE as VAT_VALUE, invhdr.BASIC_TOTAL as
       BASIC_VALUE,invhdr.PDC_CHARGES pdc_charge,
       invhdr.SUB_TOTAL as SUB_TOTAL,invhdr.DISCOUNT_VALUE DISCOUNT_VALUE ,invhdr.DIS_TOTAL DIS_TOTAL,invhdr.SUB_TAX_TOTAL as SUB_TAX_TOTAL,invhdr.GRAND_TOTAL as GRAND_TOTAL,invhdr.DUTY_EXEMPTED as DUTY_EXEMPTED,invhdr.CFORM_REQD,invhdr.FREIGHT_VALUE as FREIGHT_CHARGE,

       invdtl.INVOICE_QTY as qty, invdtl.UNIT_RATE as unit_price, invdtl.BASIC_VALUE as value, cust.CLIENT_NAME as customer_name, cust.CLNT_ADDRESS as address1,
       cust.phone_no as telephone, cust.post_code as zipcode,city.CITY_NAME as city_name, state.STATE_NAME as state_name,
       ctry.COUNTRY_NAME as country_name, invhdr.FREIGHT_TERMS as freight_desc, invdtl.CUST_PROD_CODE prod_name, invdtl.CUST_FLD1 PART_NO, pohead.PO_NO as PO_NUMBER,pohead.PO_DATE PO_DATE,
       comp.ECC_NO saarcecc, comp.TIN_NO saarctin, comp.CST_NO saarccst, comp.PAN_NO saarcpan,

       ltrim(pkg_app_utils.fn_amount_to_words(round(invhdr.ED_VALUE+invhdr.HED_VALUE+invhdr.CESS_VALUE,2))) duty_amt_words,
       ltrim( pkg_app_utils.fn_amount_to_words(round(invhdr.GRAND_TOTAL,2))) gtotal_amt_words,
       nvl(invdtl.CUST_FLD1,'---') saarc_part_no, nvl(invdtl.CUST_FLD1,'---') cus_part_no,
       invhdr.CARRIER trans_name,nvl(invhdr.LR_NO,'---') lr_no, nvl(invhdr.DELI_NOTE_NO,'---') del_note_no,
       nvl(invhdr.DESPATCHED_BY,'--') despatch_by,invhdr.OTHERS_VALUE othercharge,comp.TIN_NO TIN,comp.CST_NO CSTNO,invhdr.PREPN_DTTIME PREPN_DTTIME,invhdr.SHIPMNT_DTTIME SHIPMNT_DTTIME

 from T_SMS_INVOICE_HDR invhdr, T_SMS_INVOICE_DTL invdtl, M_CRM_CLIENTS cust, M_BASE_CITY city,M_BASE_STATE state,
      M_BASE_COUNTRY ctry, M_BASE_PRODUCT prd,T_SMS_PO_HDR pohead, M_BASE_COMPANY comp

where invhdr.INVOICE_ID = invdtl.INVOICE_ID
  and invhdr.CLIENT_ID = cust.CLIENT_ID
  and invhdr.PO_ID = pohead.PO_ID
  and comp.COMPANY_ID = 1
  and invdtl.INVDTL_STATUS = 'Active'
 and  cust.CITY_ID = city.CITY_ID(+)
  and  cust.STATE_ID = state.STATE_ID
  and  cust.COUNTRY_ID = ctry.COUNTRY_ID
    and invdtl.PRODUCT_ID = prd.PRODUCT_ID
  and invhdr.INVOICE_ID  = $P{INVHDR}]]>
	</queryString>
